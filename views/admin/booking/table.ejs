
        <div class="row">
            <div class="col-lg-12 col-md-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <div class="d-flex justify-content-between">
                            <p class="card-title">List <%- title %></p>
                            <a href="/booking/create" class="btn btn-outline-info btn-fw">Tambah <%- title %></a>
                        </div>
                        <br>
                        <div class="table-responsive">
                        <table class="table">
                            <thead>
                            <tr>
                                <th>No</th>
                                <th>User</th>
                                <th>Item</th>
                                <th>Tgl. Pinjam</th>
                                <th>Tgl. Pengembalian</th>
                                <th>Aksi</th>
                            </tr>
                            </thead>
                            <tbody>
                                <% for (let i = 0; i < booking.length; i++){%>
                                    <tr>
                                        <td><%= i + 1%></td>
                                        <td>
                                            <% booking[i].user.forEach(users => { %>
                                                <%= users.name%>
                                            <%}) %>
                                        </td>
                                        <td>
                                            <% booking[i].item.forEach(items => { %>
                                                <%= items.title%>
                                            <%}) %>
                                        </td>
                                        <td><%= booking[i].start_date%></td>
                                        <td><%= booking[i].end_date%></td>
                                        <td><%= booking[i].status%></td>
                                        <td>
                                            <form action="/booking/delete/<%= booking[i]._id %>?_method=DELETE"  method="POST">
                                                <button type="submit" class="btn btn-sm btn-danger btn-rounded btn-icon">
                                                    <i class="ti-trash"></i>
                                                </button>
                                            </form>
                                        </td>
                                    </tr>
                                <%}%>
                            </tbody>
                        </table>
                        </div>
                    </div>
                    </div>
                </div>
        </div>